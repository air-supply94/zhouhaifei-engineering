{
  "name": "@zhouhaifei/postcss-preset",
  "version": "3.5.1",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "postcss": "8.4.47",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "10.0.9",
    "tailwindcss": "3.4.14"
  },
  "devDependencies": {
    "tsup": "8.1.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
